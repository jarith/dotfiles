[
    // File / command palette / quick open
    {
        "key": "shift shift",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "cmd+p",
        "command": "workbench.action.quickOpen"
    },
    // Editor
    {
        "key": "cmd+e",
        "command": "editor.action.marker.next",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+cmd+e",
        "command": "editor.action.marker.prev",
        "when": "editorTextFocus"
    },
    // Sidebar focus
    {
        "key": "s h",
        "command": "workbench.action.focusSideBar",
        "when": "editorTextFocus && vim.mode == 'Normal'"
    },
    {
        "key": "s l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "(sideBarFocus && !inputFocus) && vim.mode == 'Normal'"
    },
    {
        "key": "n",
        "command": "explorer.newFile",
        "when": "sideBarFocus && !inputFocus"
    },
    {
        "key": "d",
        "command": "explorer.newFolder",
        "when": "sideBarFocus && !inputFocus"
    },
    {
        "key": "ctrl+d",
        "command": "deleteFile",
        "when": "sideBarFocus && !inputFocus"
    },
    // Clojure
    {
        "key": "cmd+[",
        "command": "calva.evaluateCurrentTopLevelForm",
        "when": "editorTextFocus && editorLangId == 'clojure'"
    },
    {
        "key": "cmd+l",
        "command": "calva.loadFile",
        "when": "editorTextFocus && editorLangId == 'clojure'"
    },
    // File view explorer
    {
        "key": "ctrl+e",
        "command": "workbench.view.explorer",
        "when": "searchViewletVisible || !sideBarVisible || (sideBarVisible && activeViewlet != 'workbench.view.explorer')"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible && activeViewlet == 'workbench.view.explorer'"
    },
    {
        "key": "ctrl+j",
        "command": "search.focus.nextInputBox",
        "when": "searchViewletVisible && searchInputBoxFocus"
    },
    {
        "key": "cmd+[",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "cmd+]",
        "command": "workbench.action.navigateForward"
    },
    // Git
    {
        "key": "cmd+b",
        "command": "git.rebase"
    }
    // Default removals
    {
        "key": "cmd+e",
        "command": "-actions.findWithSelection"
    },
    {
        "key": "cmd+k e",
        "command": "-workbench.files.action.focusOpenEditorsView",
        "when": "workbench.explorer.openEditorsView.active"
    },
    {
        "key": "cmd+; cmd+e",
        "command": "-testing.debugFailTests"
    },
    {
        "key": "cmd+; e",
        "command": "-testing.reRunFailTests"
    },
    {
        "key": "cmd+e",
        "command": "-editor.action.toggleScreenReaderAccessibilityMode",
        "when": "accessibilityHelpIsShown"
    },
    {
        "key": "shift+cmd+e",
        "command": "-workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "cmd+k cmd+e",
        "command": "-keybindings.editor.defineWhenExpression",
        "when": "inKeybindings && keybindingFocus"
    },
    {
        "key": "ctrl+shift+alt+e",
        "command": "-paredit.wrapAroundSet",
        "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure' && paredit:keyMap =~ /original|strict/"
    },
    {
        "key": "ctrl+alt+space e",
        "command": "-calva.runCustomREPLCommand",
        "when": "calva:connected && calva:keybindingsEnabled"
    },
    {
        "key": "ctrl+alt+c ctrl+alt+e",
        "command": "-calva.sendCurrentFormToOutputWindow",
        "when": "calva:connected && calva:keybindingsEnabled && !calva:outputWindowActive && editorLangId == 'clojure'"
    },
    {
        "key": "ctrl+e",
        "command": "-cursorLineEnd",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-cursorLineEndSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+e",
        "command": "-extension.vim_ctrl+e",
        "when": "editorTextFocus && vim.active && vim.use<C-e> && !inDebugRepl"
    },
    {
        "key": "ctrl+end",
        "command": "-extension.vim_ctrl+end",
        "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Insert'"
    },
    // Editor
    {
        "key": "cmd+e",
        "command": "editor.action.marker.next",
        "when": "editorTextFocus"
    }
]